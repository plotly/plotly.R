Package: plotly
Type: Package
Title: Interactive, publication-quality graphs online.
Version: 0.5.22
Authors@R: c(person("Chris", "Parmer", role = c("aut", "cre"),
    email = "chris@plot.ly"),
    person("Scott", "Chamberlain", role = "aut",
    email = "myrmecocystus@gmail.com"),
